@model IEnumerable<MVC_DziennikSzkolny.Models.ListaNauczycieliPrzedmiotu>

@{
    ViewBag.Title = "Przedmioty";
}
@{
    Layout = "~/Views/Nauczyciel/_LayoutPanel.cshtml";
}
<h2>Przedmioty</h2>


<table class="table">
   

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.przedmiot.nazwa)
            </td>
            <td>
                @Html.ActionLink("Details", "DetailsPrzedmiot", new { id = item.przedmiotID }) |
              
            </td>
        </tr>
    }

</table>
<script>
    $(document).ready(function () {
        $("#menuPrzedmioty").addClass("active");
    });
</script>